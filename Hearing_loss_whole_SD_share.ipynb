   "source": [
    "## filter out those diagnosis codes matched to the exclusion area of the target phenotype\n",
    "\n",
    "# load icd9/10/snomed-ct to phemap mapping\n",
    "phecode_icd_map = np.load('./data/phecodes_cm_rolled_format_corrected.npy', allow_pickle=True).item()\n",
    "\n",
    "row_index_to_remove = []\n",
    "icd9_code_to_remove = []\n",
    "icd10_code_to_remove = []\n",
    "for i in range(len(tf_idf_mapped_concept_phenotype_top)):\n",
    "    ind, phecode, _, STR, SAB, CODE, _, _ = tf_idf_mapped_concept_phenotype_top.iloc[i]\n",
    "    phecode_mapped = False\n",
    "    if SAB == 'ICD10CM' or SAB == 'ICD9CM' or SAB == 'SNOMEDCT_US':\n",
    "        if (CODE, SAB) in phecode_icd_map.keys():\n",
    "            phecode_mapped = True\n",
    "            all_mapped_phecodes = phecode_icd_map[(CODE, SAB)]\n",
    "            # determine whether the considered code can be mapped to the exclusion ranges of 389.1\n",
    "            if '389.1' not in all_mapped_phecodes:\n",
    "                within_exclusion_range = False\n",
    "                for mapped_phecode in all_mapped_phecodes:\n",
    "                    if float(mapped_phecode) >= 387.0 and float(mapped_phecode) <= 389.99:\n",
    "                        within_exclusion_range = True\n",
    "                        break\n",
    "                if within_exclusion_range == True:\n",
    "                    row_index_to_remove.append(i)\n",
    "                    print(SAB, CODE, STR)\n",
    "                    \n",
    "tf_idf_mapped_concept_phenotype_top = tf_idf_mapped_concept_phenotype_top.drop(tf_idf_mapped_concept_phenotype_top.index[row_index_to_remove])\n",
    "tf_idf_mapped_concept_phenotype_top = tf_idf_mapped_concept_phenotype_top.reset_index(drop=True)\n",
    "tf_idf_mapped_concept_phenotype_top.to_csv('./data/PheMAP_389.1_refined.csv', index=False)\n",
    "tf_idf_mapped_concept_phenotype_top"
   ]
