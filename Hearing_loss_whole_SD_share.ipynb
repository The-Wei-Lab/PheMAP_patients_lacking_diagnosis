    "ICD_diagnosis_code_not_in_PheMAP = []\n",
    "tmp = patient_concept_merge_clean.groupby(['PERSON_ID'])\n",
    "p_ind = 0\n",
    "for key, item in tmp:\n",
    "    if key not in patient_demographics['PERSON_ID']:\n",
    "        p_ind += 1\n",
    "        continue\n",
    "\n",
    "    group_df = tmp.get_group(key)\n",
    "    group_df = group_df.reset_index(drop=True)\n",
    "    group_df = group_df.drop_duplicates()\n",
    "    group_df = group_df.reset_index(drop=True)\n",
    "\n",
    "    p_id = group_df.loc[0]['PERSON_ID']\n",
    "    item_recall = 0\n",
    "    missing_concept = []\n",
    "    matched_concept = []\n",
    "    concept_associated = []\n",
    "    tfidf_score = 0\n",
    "    has_ICD_diagnosis_code = False\n",
    "    ICD_diagnosis_code_list = []\n",
    "\n",
    "    for raw_table in group_df.values.tolist():\n",
